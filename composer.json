{
    "name": "webignition/tcp-cli-proxy-client",
    "description": "",
    "keywords": [],
    "homepage": "https://github.com/webignition/tcp-cli-proxy-client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "webignition@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "webignition\\TcpCliProxyClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "webignition\\TcpCliProxyClient\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs": [
            "./vendor/bin/ecs check src tests --ansi --config ecs.yaml",
            "./vendor/bin/phpcs"
        ],
        "cs-fix": [
            "./vendor/bin/ecs check src tests --ansi --config ecs.yaml --fix"
        ],
        "static-analysis": "./vendor/bin/phpstan analyse --ansi",
        "test": "./vendor/bin/phpunit --colors=always",
        "ci": [
            "@cs",
            "@static-analysis",
            "@test"
        ],
        "server-fetch": [
            "rm -f server.phar",
            "curl -L https://github.com/webignition/tcp-cli-proxy-server/releases/download/0.5/server.phar --output server.phar",
            "chmod +x server.phar"
        ],
        "client-test": [
            "./vendor/bin/phpunit --colors=always tests/Integration/ClientTest.php"
        ]
    },
    "require": {
        "php": ">=7.4",
        "webignition/error-handler": "^0.1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "php-mock/php-mock": "^2.2",
        "php-mock/php-mock-mockery": "^1.3",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.0",
        "phpstan/phpstan-mockery": "^0.12.7",
        "phpunit/phpunit": "^9.2",
        "squizlabs/php_codesniffer": "3.*",
        "symplify/easy-coding-standard": "^8.0",
        "webignition/object-reflector": "^0.2.0"
    },
    "config": {
        "sort-packages": true
    }
}
